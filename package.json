{
  "name": "social-media-app",
  "version": "1.0.0",
  "description": "Social Media Application API - Node.js Backend",
  "main": "app.js",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "start": "node app.js",
    "dev": "nodemon app.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "jest --coverage",
    "db:migrate": "mysql -u $DB_USER -p$DB_PASSWORD $DB_NAME < db/migrations.sql",
    "db:create": "mysql -u $DB_USER -p$DB_PASSWORD -e 'CREATE DATABASE IF NOT EXISTS $DB_NAME'",
    "db:drop": "mysql -u $DB_USER -p$DB_PASSWORD -e 'DROP DATABASE IF EXISTS $DB_NAME'"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "keywords": [
    "social-media",
    "nodejs",
    "express",
    "api"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.24.0",
    "body-parser": "^1.19.0",
    "console-stamp": "^0.2.9",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "csv-parse": "^5.5.3",
    "dotenv": "^16.0.1",
    "eb-butler-utils": "^1.0.11",
    "ejs": "^3.1.6",
    "express": "^4.17.1",
    "express-validator": "^6.14.1",
    "jsonwebtoken": "^9.0.2",
    "mime-types": "^2.1.28",
    "moment": "^2.29.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "mysql2": "^2.2.5",
    "node-cryptojs-aes": "^0.4.0",
    "nodemailer": "^6.9.1",
    "nodemon": "^2.0.7",
    "sequelize": "^6.5.0",
    "stripe": "^18.3.0",
    "uuid": "^8.3.2"
  }
}